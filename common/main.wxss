view,
text,
navigator,
input,
image,
swiper,
swiper-item,
scroll-view,
page {
  /* 微信小程序中 不支持*通配符 */
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
page {
  font-size: 28rpx;
  color: #666666;
}
image {
  width: 100%;
  display: block;
}
@font-face {
  font-family: 'iconfont';
  /* project id 1796536 */
  src: url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.eot');
  src: url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.eot?#iefix') format('embedded-opentype'),
    url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.woff2') format('woff2'),
    url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.woff') format('woff'),
    url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.ttf') format('truetype'),
    url('https://at.alicdn.com/t/font_1796536_7bmptiky5mr.svg#iconfont') format('svg');
}
text[class*="icon-"],
view[class*="icon-"],
button[class*="icon-"] {
  font-family: "iconfont";
  font-size: inherit;
  font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
